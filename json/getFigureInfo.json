---
layout: none
---    
{
    "figureInfo" : [
        {% assign figures = site.figures | sort: 'figid_alias' %}
        {% for pw in figures %}
            {
                "figid": "{{ pw.figid }}",
                "url": "https://pfocr.wikipathways.org/figures/{{ pw.figid_alias }}",
                "pmcid": "{{ pw.pmcid }}",
                "number": "{{ pw.number }}",
                "figtitle": "{{ pw.figtitle | escape }}",
                "year": "{{ pw.year }}",
                "organisms": "{{ pw.organisms | join: ', ' }}",
                "organisms_ner": "{{ pw.organisms_ner | join: ', ' }}"
            }{% unless forloop.last %},{% endunless %}
        {% endfor %}
    ]
} 
